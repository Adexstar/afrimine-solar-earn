{
  "name": "afrimine-monorepo",
  "private": true,
  "packageManager": "pnpm@9",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter web dev",
    "dev:native": "pnpm --filter native start",
    "build:dev": "vite build --mode development",
    "install-all": "pnpm -w install"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
